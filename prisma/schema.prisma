generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

enum Status {
  PENDING
  APPROVED
  BLOCKED
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  company   String?
  entryId   Int      @unique
  status    Status   @default(PENDING)
  createdAt DateTime @default(now())
}